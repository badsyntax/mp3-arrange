{
  "name": "mp3-tools",
  "version": "0.0.1",
  "description": "A script to arrange your mp3 collection",
  "private": "true",
  "scripts": {
    "prewatch": "npm install",
    "pretest": "npm install",
    "pretest-coverage": "npm run gen-coverage",
    "precoveralls": "npm run gen-coverage",
    "test-with-coverage": "npm run test-coverage",
    "test": "mocha spec --recursive --ui bdd -t 5000",
    "test-watch": "mocha spec --recursive --ui bdd -t 5000 --watch",
    "watch": "npm run test-watch",
    "gen-coverage": "istanbul cover ./node_modules/.bin/_mocha -- spec --recursive --ui bdd -t 5000",
    "test-coverage": "istanbul check-coverage coverage/coverage.json --statements 90 --branches 50 --lines 90 --functions 90",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "author": "Richard Willis <willis.rh@gmail.com>",
  "license": "MIT",
  "bin": {
    "mp3-arrange": "bin/mp3-arrange"
  },
  "preferGlobal": "true",
  "dependencies": {
    "async": "^0.9.0",
    "bytes": "^1.0.0",
    "charm": "^1.0.0",
    "fs-extra": "^0.16.3",
    "glob": "^4.3.5",
    "lodash": "^3.1.0",
    "musicmetadata": "^0.6.6",
    "nomnom": "^1.8.1",
    "numeral": "^1.5.3",
    "winston": "^0.9.0"
  },
  "devDependencies": {
    "chai": "^1.10.0",
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.5",
    "mocha": "^2.1.0"
  }
}
